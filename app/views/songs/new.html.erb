<h1>New Song Form</h1>

<%= form_for @song do |f| %>
    <%= f.label 'Song Name' %><br>
    <%= f.text_field :name %><br>

    <%= f.label 'Artist Name' %><br>
    <%= f.text_field :artist_id %><br>

    <%= f.label 'Genre' %><br>
    <%= f.text_field :genre_id %><br>

    <%= f.submit "Create Song" %>
<% end %>